<%- include('../partials/header') %>

<h1>
  <%= title %>
</h1>
<section id="show-page">
  <div>Name: 
    <%= camp.name %></div>
  <div>Description: 
    <%= camp.description %></div>
  <div>Total Sites: 
    <%= camp.totalSites %></div>
  <div>Location
    <%= camp.location %> </div>
  <div>Noise Level
    <%= camp.noiseLevel %></div>
  <div>RV's Permitted:
    <%= camp.rvPermitted ? 'Yes' : 'No' %></div>
</section>

<form action="/camps/<%=camp._id %>?_method=DELETE" method="POST">
  <button>DELETE</button>
</form>

<a href="/camps/<%=camp._id%>/edit">EDIT</a>
<%- include('../partials/footer') %>
